<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.guru.muhwezidenisliam.course_gudie" >

    <uses-permission android:name="ccom.guru.muhwezidenisliam.course_gudie.permission.MAPS_RECEIVE" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />


    <application
        android:allowBackup="true"
        android:largeHeap="true"
        android:icon="@drawable/courseguide"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />


        <activity android:name=".activity_classes.Home"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity_classes.About"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
            />

        <activity android:name=".activity_classes.O_level"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>

        <activity android:name=".activity_classes.Help"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>

        <activity android:name=".activity_classes.Cal_Weight"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>

        <activity android:name=".activity_classes.A_level"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>

        <activity android:name=".activity_classes.Find_Courses"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>

        <activity android:name=".activity_classes.Courses"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>

        <activity android:name=".activity_classes.Directions"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>

        <activity android:name=".activity_classes.News"
            android:theme="@style/AppTheme"/>

    </application>

</manifest>